{
  "name": "win-ca",
  "private": true,
  "version": "2.4.0",
  "description": "Make Trusted Root Certificates @Windows available to VSCode extensions",
  "icon": "win-ca.png",
  "main": "index.js",
  "engines": {
    "vscode": "^1.0.0"
  },
  "publisher": "ukoloff",
  "repository": {
    "type": "git",
    "url": "git+https://ukoloff@github.com/ukoloff/win-ca.git"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "win-ca",
      "properties": {
        "win-ca.save": {
          "type": "boolean",
          "default": true,
          "description": "Save fetched root certificates to disk"
        }
      }
    }
  },
  "scripts": {},
  "keywords": [
    "vscode",
    "ssl",
    "ca",
    "root",
    "windows"
  ],
  "author": {
    "name": "Stas Ukolov",
    "email": "ukoloff@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "win-ca": "^2.4.0"
  }
}
